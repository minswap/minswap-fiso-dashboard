    "classnames": "^2.3.1",
    "crypto-random-string": "^4.0.0",
    "framer-motion": "^4.1.17",
    "next": "^12.0.7",
    "qr-code-styling": "^1.6.0-rc.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-popper": "^2.2.5"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.1.0",
    "@next/bundle-analyzer": "^12.0.7",
    "@types/node": "^14.17.9",
    "@types/react": "^17.0.16",
